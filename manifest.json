{
	"manifest_version": 2,
	"name": "gh-light-ui",
	"version": "1.0.0",
	"description": "Make GitHub Great Again",
	"homepage_url": "https://github.com/karlerikjonatan/gh-light-ui",
	"content_scripts": [
		{
			"matches": [
				"*://*.github.com/*"
			],
			"js": ["gh-light-ui.js"],
			"run_at": "document_start"
		}
	]
}
